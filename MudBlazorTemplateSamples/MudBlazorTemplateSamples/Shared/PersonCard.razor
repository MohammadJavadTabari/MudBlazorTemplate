<MudCard Elevation="0" Square="true" Class="@Class" Style="@Style">
    <MudCardHeader>
        <CardHeaderAvatar>
            <MudAvatar Image="images/MohammadJavadTabari1024.jpeg" />
        </CardHeaderAvatar>
        <CardHeaderContent>
            <MudText Typo="Typo.body2">Illegible</MudText>
            <MudText Typo="Typo.caption">Addicted To Coding </MudText>
        </CardHeaderContent>
    </MudCardHeader>
</MudCard>

@code {
    [Parameter] public string Class { get; set; }
    [Parameter] public string Style { get; set; }
}